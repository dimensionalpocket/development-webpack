{
  "name": "@dimensionalpocket/development-webpack",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test c8 mocha",
    "test:ci": "NODE_ENV=test mocha",
    "lint": "standardx --fix --verbose",
    "release": "release-it --config ./node_modules/@dimensionalpocket/development/release-it/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimensionalpocket/development-webpack.git"
  },
  "author": "dimensionalpocket.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimensionalpocket/development-webpack/issues"
  },
  "homepage": "https://github.com/dimensionalpocket/development-webpack#readme",
  "dependencies": {
    "@babel/preset-env": "7.15.6",
    "@dimensionalpocket/development": "github:dimensionalpocket/development-js#0.4.0",
    "babel-loader": "8.2.2",
    "css-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "sass": "1.39.2",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "webpack": "5.52.1",
    "webpack-dev-server": "3.11.2"
  },
  "babel": {
    "extends": "./node_modules/@dimensionalpocket/development/babel"
  },
  "eslintConfig": {
    "extends": "./node_modules/@dimensionalpocket/development/eslint"
  },
  "mocha": {
    "recursive": true,
    "reporter": "dot",
    "colors": true
  },
  "c8": {
    "all": true,
    "include": [
      "index.js",
      "rules"
    ]
  },
  "engines": {
    "node": ">= 14.17.1"
  },
  "devDependencies": {
    "release-it": "14.11.6"
